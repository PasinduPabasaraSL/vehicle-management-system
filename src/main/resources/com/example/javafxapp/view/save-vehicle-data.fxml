<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxapp.controller.SaveVehicleDataController">
   <children>
      <Label layoutX="216.0" layoutY="35.0" text="Vehicle Data" textFill="#d9dad2">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="txtId" layoutX="142.0" layoutY="104.0" prefHeight="25.0" prefWidth="310.0" promptText="Enter Vehicle Id">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtBrand" layoutX="142.0" layoutY="147.0" prefHeight="25.0" prefWidth="310.0" promptText="Enter Brand">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtModel" layoutX="142.0" layoutY="185.0" prefHeight="25.0" prefWidth="310.0" promptText="Enter Model">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtQty" layoutX="142.0" layoutY="227.0" prefHeight="25.0" prefWidth="310.0" promptText="Enter qty">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtPrice" layoutX="142.0" layoutY="268.0" prefHeight="25.0" prefWidth="310.0" promptText="Enter Price">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button layoutX="142.0" layoutY="332.0" mnemonicParsing="false" onAction="#save" style="-fx-background-color: green;" text="Save" textFill="#f8f5f5">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="383.0" layoutY="332.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #e51818;" text="Back" textFill="#f8f6f6">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
